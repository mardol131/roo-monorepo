{
  "name": "@test/email-builder",
  "version": "1.1.0",
  "private": false,
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.js"
    }
  },
  "type": "module",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "cd src && email dev -p 3002",
    "export": "cd src && email export",
    "build": "tsc --project tsconfig.build.json",
    "type-check": "tsc --noEmit"
  },
  "dependencies": {
    "@react-email/components": "1.0.3",
    "react": "^19.2.1",
    "react-dom": "^19.2.1"
  },
  "devDependencies": {
    "@react-email/preview-server": "5.2.1",
    "@types/react": "^19.0.1",
    "@types/react-dom": "^19.0.1",
    "react-email": "5.2.1",
    "typescript": "5.7.3"
  }
}
